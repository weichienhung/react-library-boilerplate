{
  "name": "my-react-library",
  "version": "1.0.0",
  "description": "boilerplate for creating react component library",
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "samuel hung",
  "license": "MIT",
  "keywords": [],
  "options": {
    "mocha": "--require scripts/mocha_runner src/**/__tests__/**/*.js"
  },
  "module": "./dist/my-react-library.js",
  "scripts": {
    "prepublish": "npm run build",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "dev": "cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && webpack --config webpack.dist.config.js",
    "testonly": "mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run test-report",
    "test-report": "cross-env NODE_ENV=test nyc --report-dir=artifacts/coverage --reporter=lcov --reporter=text mocha --require scripts/mocha_runner src/**/__tests__/**/*.js",
    "test-watch": "npm run testonly -- --watch --watch-extensions js"
  },
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.23.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": ">=4.18.2",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.2.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "fbjs": "^0.8.16",
    "image-webpack-loader": "^3.3.0",
    "jsdom": "^8.1.0",
    "mocha": "^5.0.0",
    "nodemon": "^1.9.1",
    "nyc": "^11.3.0",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-hot-loader": "^3.1.3",
    "sinon": "^1.17.3",
    "styled-components": "^3.1.5",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^3.1.11"
  },
  "peerDependencies": {
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "fbjs": "^0.8.16",
    "prop-types": "^15.6.0",
    "styled-components": "^3.1.5"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1"
  }
}
